{
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": true,
    "baseUrl": "./src",
    "checkJs": false,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "build",
    "rootDir": "./src",
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "target": "esnext",
    "types": [
      "node",
      "jest"
    ]
  }
}
